 allUrls = await getImageUrlsFromStorage();\r\n        setUrls(allUrls);\r\n        console.log(\"URLS: \", urls);\r\n        setImagesLoading(false);\r\n    }\r\n\r\n    const handleSelect = (event) => {\r\n        setCategory(event.target.value);\r\n    }\r\n\r\n    // Similar ao componentDidMount e componentDidUpdate:\r\n    useEffect(() => {\r\n        document.title = \"SOFTWAVE\";\r\n        loadImages();\r\n    }, [category]); // vai entrar em ação assim que a categoria mudar.\r\n\r\n    if(imagesLoading) {\r\n        return (\r\n            <Container>\r\n                    <Loader\r\n                        type=\"Oval\"\r\n                        color={COLORS.rosa}\r\n                        height={100}\r\n                        width={100}\r\n                    />\r\n            </Container>\r\n        );\r\n    }\r\n\r\n    return (\r\n        <Container>   \r\n            {\r\n                imagesExist ?\r\n                    <WindowsGallery\r\n                        icon={folderIcon}\r\n                        title=\"Exploring - Images\\Categoria\"\r\n                        category={category}\r\n                        onPress={handleSelect}\r\n                    >\r\n                        <Grid urls={urls}/>\r\n                    </WindowsGallery>\r\n                :\r\n                    <Text>Ainda não foram adicionadas imagens.</Text>         \r\n            }\r\n        </Container>\r\n    );\r\n}\r\n\r\nexport default Home;","C:\\Users\\Windows 10\\Documents\\softwave\\src\\components\\WindowsButton\\index.js",[],"C:\\Users\\Windows 10\\Documents\\softwave\\src\\components\\WindowsButton\\styles.js",[],"C:\\Users\\Windows 10\\Documents\\softwave\\src\\components\\Footer\\index.js",["132"],"C:\\Users\\Windows 10\\Documents\\softwave\\src\\components\\Footer\\styles.js",[],"C:\\Users\\Windows 10\\Documents\\softwave\\src\\pages\\FileUploader\\index.js",[],"C:\\Users\\Windows 10\\Documents\\softwave\\src\\pages\\FileUploader\\style