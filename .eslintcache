 },\r\n            error => {\r\n                console.log(error);\r\n            },\r\n            () => {\r\n                storage\r\n                    .ref(\"images\")\r\n                    .child(image.name)\r\n                    .getDownloadURL()\r\n                    .then(url => {\r\n                        setUrl(url);\r\n                        alert(\"Successful\");\r\n                    })\r\n            }\r\n        )\r\n    };\r\n\r\n    console.log(\"image -> \", image);\r\n    return (\r\n        <Container>\r\n\r\n            <Box>\r\n                \r\n                <h1>Upload image</h1>\r\n                <p>Choose an awesome image to upload to Softwave!</p>\r\n                \r\n                <img style={{padding: 30, width: '50%'}} src={url || \"https://i.imgur.com/YTmPA0u.jpg\"} alt=\"firebase-image\"/>\r\n                \r\n                <Row>\r\n                    {!url && <progress value={progress} style={{marginLeft: 10, width: 250}} max=\"100\"/>}\r\n                </Row>\r\n\r\n                <Row>\r\n                    <FileInput>\r\n                        <input type=\"file\" onChange={handleChange}/>\r\n                    </FileInput>\r\n\r\n                   <WindowsButton texto=\"Enviar\" onPress={handleUpload}/>\r\n                </Row>\r\n                \r\n            </Box>\r\n            \r\n        </Container>\r\n    )\r\n}\r\n\r\nexport default FileUploader;","C:\\Users\\Windows 10\\Documents\\softwave\\src\\pages\\FileUploader\\styles.js",[],"C:\\Users\\Windows 10\\Documents\\softwave\\src\\components\\Frame\\index.js",[],["112","113"],"C:\\Users\\Windows 10\\Documents\\softwave\\src\\components\\Frame\\styles.js",[],"C:\\Users\\Windows 10\\Documents\\softwave\\src\\pages\\Home\\styles.js",[],"C:\\Users\\Windows 10\\Documents\\softwave\\src\\pages\\About\\index.js",[],"C:\\Users\\Windows 10\\Documents\\softwave\\src\\pages\\About\\styles.js",[],"C:\\Users\\Windows 10\\Documents\\softwave\\src\\fire